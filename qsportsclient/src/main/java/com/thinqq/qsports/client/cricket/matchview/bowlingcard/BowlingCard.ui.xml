<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns= 'com.thinqq.qsports.client.cricket.matchview.bowlingcard'
	xmlns:q='urn:import:com.thinqq.qsports.client.cricket.matchview.bowlingentry'
	xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:VerticalPanel styleName="greypanel" ui:field="bowlingCard" width="420px">
		<g:Cell width="100%">
			<g:HorizontalPanel>
				<g:Label ui:field="teamName" styleName="teamname"
					text="Weekend Warriors's Bowling" />
			</g:HorizontalPanel>
		</g:Cell>
		<g:VerticalPanel ui:field="bowlingEntries" width="400px">
			<g:SimplePanel width="100%" styleName="simple_heading_panel">
				<g:HorizontalPanel ui:field="bowlingEntryHeading"
					width="380px" height="30px" verticalAlignment="ALIGN_MIDDLE"
					horizontalAlignment="ALIGN_LEFT">
					<g:Cell width="100px">
						<g:Label text="Batsman" styleName="simple_heading_label"
							horizontalAlignment="ALIGN_LEFT" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Overs" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Mdns" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Runs" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Wkts" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Econ" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Wide" />
					</g:Cell>
					<g:Cell width="40px" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="NoBall" />
					</g:Cell>
				</g:HorizontalPanel>
			</g:SimplePanel>
			<q:BowlingEntry></q:BowlingEntry>
			<q:BowlingEntry></q:BowlingEntry>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
