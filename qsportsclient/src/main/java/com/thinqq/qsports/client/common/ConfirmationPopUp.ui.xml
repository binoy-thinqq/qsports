<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel width="80%">
		<table>
			<tr>
				<td width="98%">
					<g:Label stylePrimaryName="subtitle" ui:field="title" text=""></g:Label>
				</td>
			</tr>
			<tr>
				<td width="98%">
					<g:Label ui:field="message" text=""></g:Label>
				</td>
			</tr>
			<tr>
				<td width="90%" style="text-align:right">
					<g:Button ui:field="ok" stylePrimaryName="gbuttonSmall"
						text="Confirm"></g:Button>
				</td>
				<td width="10%">
					<g:Button ui:field="cancel" stylePrimaryName="gbuttonSmall"
						text="Cancel"></g:Button>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 