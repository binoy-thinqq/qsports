<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel width="100%" height="600px">
					<br></br>
					<g:Grid width="100%" height="600px">
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Your Team"/>
							</g:customCell>
							<g:customCell>
								<g:HTMLPanel ui:field="myteampanel"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Opponent Team"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="oppteam"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Venue"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="venue"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Location"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="location"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Date"/>
							</g:customCell>
							<g:customCell>
							<g:HTMLPanel>
							<g:TextBox ui:field="matchdate"/>
							<g:Button ui:field="pickdate">Pick Date!</g:Button>
							</g:HTMLPanel>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Format"/>
							</g:customCell>
							<g:customCell>
								<g:ListBox width="150px" ui:field="format"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Notification(Request for Approval) "/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="email"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:InlineLabel text="Contact"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="contact"/>
							</g:customCell>
						</g:row>
					</g:Grid>
					
				</g:HTMLPanel>
</ui:UiBinder> 